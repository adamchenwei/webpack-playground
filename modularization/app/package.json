{
  "name": "app",
  "version": "0.0.2",
  "description": "app test",
  "main": "dist/index.js",
  "scripts": {
    "app": "npm-run-all --parallel dev analyze",
    "prod": "NODE_ENV=production npm run p && npm run analyze",
    "prod:win": "set NODE_ENV=production npm run p && npm run analyze",
    "dev": "webpack-dev-server",
    "p": "webpack -p",
    "paw": "webpack -p && npm run analyze:win",
    "pam": "webpack -p && npm run analyze",
    "start": "webpack --env.watch=true",
    "start:p": "webpack -p --env.watch=true",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -s ./public -p 9001",
    "build-storybook": "build-storybook",
    "analyze": "webpack --profile --json > stats.json",
    "analyze:win": "webpack --env.analyze=true --profile --json | Out-file 'stats.json' -Encoding OEM"
  },
  "author": "Ezeewei",
  "license": "ISC",
  "devDependencies": {
    "@storybook/react": "^4.0.12",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^8.0.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "html-webpack-plugin": "^3.2.0",
    "npm-run-all": "^4.1.2",
    "storybook": "^1.0.0",
    "webpack": "^4.27.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.0.0-beta.3",
    "material-ui": "^1.0.0-beta.20",
    "material-ui-icons": "^1.0.0-beta.17",
