    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-html-parser": "^2.0.2",
    "react-images": "^0.5.14",
    "react-router": "^4.2.0",
    "react-share": "^2.4.0",
    "react-svg": "^7.2.2",
    "reactstrap": "^6.5.0",
    "styled-components": "^4.1.2"
  }
